{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "process",
      "command": "dotnet",
      "args": [
        "build",
        "${workspaceFolder}",
        "-property:GenerateFullPaths=true",
        "-consoleLoggerParameters:NoSummary;ForceConsoleColor",
        "-p:Configuration=Debug",
        "-p:Platform=\"Any CPU\"",
        "-bl"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": [],
      "label": "dotnet: build"
    }
  ]
}